{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "duration_of_cv": {
      "type": "string"
    },
    "cv_start_date": {
      "type": "datetime"
    },
    "profile_picture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "skills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::skill.skill",
      "mappedBy": "user"
    },
    "references": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reference.reference",
      "mappedBy": "user"
    },
    "educations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::education.education",
      "mappedBy": "user"
    },
    "experience_descriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::experience-description.experience-description",
      "mappedBy": "user"
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "resume": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::resume.resume"
    },
    "work_experiences": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::work-experience.work-experience",
      "mappedBy": "owner"
    }
  }
}
