{
  "kind": "collectionType",
  "collectionName": "resumes",
  "info": {
    "singularName": "resume",
    "pluralName": "resumes",
    "displayName": "Resume",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "professional_summary": {
      "type": "richtext"
    },
    "access_code": {
      "type": "integer"
    },
    "twitter_link": {
      "type": "string"
    },
    "hero_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "skills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::skill.skill",
      "mappedBy": "resume"
    },
    "references": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reference.reference",
      "mappedBy": "resume"
    },
    "educations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::education.education",
      "mappedBy": "resume"
    },
    "work_experiences": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::work-experience.work-experience",
      "mappedBy": "resume"
    },
    "title": {
      "type": "string",
      "unique": true
    },
    "linkedin_link": {
      "type": "string"
    },
    "current_position": {
      "type": "string"
    }
  }
}
